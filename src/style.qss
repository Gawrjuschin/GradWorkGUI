Results_Widget  QComboBox
{
    font: 14px;
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.5 white, stop: 1.0 lightgray);
    border: 1px solid gray;
    padding: 1px 18px 1px 3px;
    min-width: 4em;
}
Results_Widget  QComboBox:hover
{
        border-color:black;
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.5 lightgray, stop: 1.0 gray);
}

/* Активирован */
Results_Widget QComboBox:!editable:on, QComboBox::drop-down:editable:on 
{    
        background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0.0 lightgray, stop: 1.0 gray);
}

Results_Widget QComboBox:on
{ /* shift the text when the popup opens */
    padding-top: 2px;
    padding-left: 4px;

}

Results_Widget QComboBox::drop-down 
{
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 18px;
    border-left: 1px solid black;
}

Results_Widget QComboBox::down-arrow:on { /* shift the arrow when popup is open */
    top: 1px;
    left: 1px;
}

